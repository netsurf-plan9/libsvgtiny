	$CFLAGS\
	-I../../libsvgtiny/include \
	-I../../libdom/include \
	-I../../libwapcaplet/include

<../../nsportlib.mk

svgtiny.c:V:	autogenerated_colors.c

autogenerated_colors.c:
	cp ../../genfiles/libsvgtiny/autogenerated_colors.c autogenerated_colors.c

clean:V:
	rm -f $OBJ $LIB autogenerated_colors.c
