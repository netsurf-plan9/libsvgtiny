</$objtype/mkfile

LIB=libsvgtiny.$O.a
OBJ=\
	svgtiny.$O \
	svgtiny_gradient.$O \
	svgtiny_list.$O \

HFILES=\
	../include/svgtiny.h \
	svgtiny_internal.h \
	svgtiny_strings.h

<../../nsport.mk

